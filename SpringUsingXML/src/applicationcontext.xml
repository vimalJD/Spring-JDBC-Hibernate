	<bean id="address" class="com.seed.beans.Address">
		<property name="street" value="paud road" />
		<property name="city" value="pune" />
		<property name="country" value="india" />
	</bean>

	<bean id="birthDate" class="com.seed.beans.BirthDate">
		<property name="dd" value="01" />
		<property name="mm" value="03" />
		<property name="yy" value="2000" />
	</bean>

	<bean id="joinDate" class="com.seed.beans.JoinDate">
		<property name="dd" value="01" />
		<property name="mm" value="03" />
		<property name="yy" value="2015" />
	</bean>

